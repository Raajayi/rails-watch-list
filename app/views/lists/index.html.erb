<div class="container">
<h1>All Lists</h1>
<%= link_to 'Create a new list', new_list_path%>
<% @lists.each do |list| %>
  <div class="card-product">
    <%= cl_image_tag list.photo.key, height: 200, width: 200, crop: :fill%>
    <div class="card-product-infos">
      <h2><%= link_to "#{list.name}", list_path(list) %></h2>
    </div>
  </div>
<% end %>
</div>
