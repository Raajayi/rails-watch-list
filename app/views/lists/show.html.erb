<h1><%= @list.name %></h1>
<h2>Movies: </h2>

<% @list.bookmarks.each do |bookmark| %>
<%= link_to "Delete Bookmark", bookmark_path(bookmark), data: {turbo_method: :delete, turbo_confirm: "Are you certain?"}%>
<p><%= bookmark.movie.title %></p>
<% end %>


<%= link_to "Add a Bookmark", new_list_bookmark_path(@list) %>
